[Unit]
Description=ngrok Tunnel for n8n
After=n8n-docker-compose.service
Requires=n8n-docker-compose.service

[Service]
Type=simple
# Update this to your username
User=YOUR_USERNAME
# Update YOUR_NGROK_DOMAIN with your actual ngrok domain
ExecStart=/usr/local/bin/ngrok http 5678 --domain=YOUR_NGROK_DOMAIN
Restart=always
RestartSec=10s
StandardOutput=journal
StandardError=journal

[Install]
WantedBy=multi-user.target

